        "action": "cancelConfig",
        "stateName": "page({jsonName:'viewConfig'})"
    }],
    "apiCall": {
        "url": "configtwo/save",
        "params": "_id"
    },
    "preApi": {
        "url": "configtwo/getOne",
        "params": "_id"

        "type": "hidden",
        "tableRef": "_id",
        "placeholder": "Enter id",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": "",
        "model": "_id"
    },{
        "name": "Select City",
        "type": "selectFromTable",

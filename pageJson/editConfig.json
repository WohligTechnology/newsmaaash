        "model": "city",
        "placeholder": "Enter name",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": "city/getAll"
    },{
        "name": "Video Title",
        "type": "text",
        "tableRef": "videotitle",
        "model": "videotitle",
        "placeholder": "Enter videotitle",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    },{
        "name": "Video Url",
        "type": "text",
        "tableRef": "videourl",
        "model": "videourl",
        "placeholder": "Enter videourl",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    },{
        "name": "Video Thumbnail",
        "type": "image",
        "tableRef": "thumbnail",
        "model": "thumbnail",
        "validation": ["required", "trim", "unique"],
        "url": "",
        "size":"1600 x 899px"
    },{
        "name": "Home Page Banner",
        "type": "image",
        "tableRef": "homebanner",
        "model": "homebanner",
        "validation": ["required", "trim", "unique"],

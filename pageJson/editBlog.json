{
    "title": "Edit Blog",
    "jsonPage": "viewBlog",
    "desciption": "",
    "pageType": "edit",
    "urlFields": ["_id"],
    "fields": [{
        "name": "Id",
        "type": "hidden",
        "tableRef": "_id",
        "placeholder": "Enter id",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": "",
        "model": "_id"
    }, {
        "name": "City Name",
        "type": "selectFromTable",
        "option": "multiple",
        "tableRef": "city",
        "model": "city",
        "placeholder": "Enter name",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": "city/getAll"
    }, {
        "name": "Name",
        "type": "text",
        "tableRef": "name",
        "model": "name",
        "placeholder": "Enter text for home slider",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Image",
        "type": "image",
        "tableRef": "image",
        "model": "image",
        "size": "360 x 190px",
        "placeholder": "Enter Picture",
        "id": "input4",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Order",
        "type": "text",
        "tableRef": "order",
        "model": "order",
        "placeholder": "Enter order",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Status",
        "type": "select",
        "tableRef": "status",
        "model": "status",
        "placeholder": "Enter Is Home",
        "id": "input3",
        "validation": ["required", "trim", "unique"],
        "url": [{
            "value": true,
            "name": "Enable"
        }, {
            "value": false,
            "name": "Disable"
        }]
    }, {
        "name": "Banner",
        "type": "image",
        "tableRef": "banner",
        "model": "banner",
        "size": "750 x 400 px",
        "placeholder": "Enter Picture",
        "id": "input4",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Date",
        "type": "date",
        "tableRef": "date",
        "model": "date",
        "placeholder": "Enter price",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Description",
        "type": "tinymce",
        "tableRef": "description",
        "model": "description",
        "placeholder": "Enter description",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }],
    "action": [{
        "name": "submit",
        "action": "submitBlog"
    }, {
        "name": "cancel",
        "action": "cancelBlog",
        "stateName": "page({jsonName:'viewBlog'})"
    }],
    "apiCall": {
        "url": "Blog/save",
        "params": "_id"
    },
    "preApi": {
        "url": "Blog/getOne",
        "params": "_id"
    }
}
{
    "title": "Create What's New",
    "jsonPage": "viewExploreSmash",
    "desciption": "",
    "pageType": "create",
    "fields": [{
        "name": "City Name",
        "type": "selectFromTable",
        "tableRef": "city",
        "option": "multiple",
        "placeholder": "Enter name",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": "city/getAll"
    }, {
        "name": "Title",
        "type": "text",
        "tableRef": "hometext",
        "placeholder": "Enter text for home slider",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Home Page Image",
        "type": "image",
        "tableRef": "image",
        "size": "250 x 137",
        "placeholder": "Enter Picture",
        "id": "input4",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Show Home",
        "type": "select",
        "tableRef": "isHome",
        "placeholder": "Enter Is Home",
        "id": "input3",
        "validation": ["required", "trim", "unique"],
        "url": [{
            "value": true,
            "name": "Yes"
        }, {
            "value": false,
            "name": "No"
        }]
    }, {
        "name": "Status",
        "type": "select",
        "tableRef": "status",
        "placeholder": "Enter Is Home",
        "id": "input3",
        "validation": ["required", "trim", "unique"],
        "url": [{
            "value": true,
            "name": "Enable"
        }, {
            "value": false,
            "name": "Disable"
        }]
    }, {
        "name": "Order",
        "type": "text",
        "tableRef": "order",
        "placeholder": "Enter text for home slider",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Inside Page Image",
        "type": "image",
        "tableRef": "homeimage",
        "url": "",
        "placeholder": "Enter Picture",
        "id": "input4",
        "validation": ["required", "trim", "unique"],
        "size": "728 x 400"
    }, {
        "name": "Inside Page Top Banner",
        "type": "image",
        "tableRef": "banner",
        "placeholder": "Enter City Picture",
        "id": "input4",
        "validation": ["required", "trim", "unique"],
        "url": "",
        "size": "1920 x 1079px"
    }, {
        "name": "Top Banner Video Url",
        "type": "text",
        "tableRef": "video",
        "placeholder": "Enter video",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Top Banner Video Thumbnail",
        "type": "image",
        "tableRef": "videoThumbnail",
        "size": "1920 x 1079px",
        "placeholder": "Enter video",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Know more",
        "type": "text",
        "tableRef": "sublink",
        "placeholder": "Enter sublink",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Branch Package ID",
        "type": "text",
        "tableRef": "BranchPackageID",
        "placeholder": "Enter BranchPackageID",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }, {
        "name": "Description",
        "type": "tinymce",
        "tableRef": "description",
        "placeholder": "Enter description",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }],
    "action": [{
        "name": "submit",
        "action": "submitExploreSmash"
    }, {
        "name": "cancel",
        "stateName": "page({jsonName:'viewExploreSmash'})"
    }],

    "apiCall": {
        "url": "exploresmash/saveWhatsNew"
    }

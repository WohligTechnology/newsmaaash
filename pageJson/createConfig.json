{
    "title": "Create Landing Page",
    "jsonPage": "viewConfig",
    "desciption": "",
    "pageType": "create",
    "fields": [{
        "name": "Select City",
        "type": "selectFromTable",
        "option": "single",
        "tableRef": "city",
        "placeholder": "Enter name",
        "id": "input1",
        "validation": ["required", "trim", "unique"],
        "url": "city/getAll"
    },{
        "name": "Video Title",
        "type": "text",
        "tableRef": "videotitle",
        "placeholder": "Enter videotitle",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    },{
        "name": "Video Url",
        "type": "text",
        "tableRef": "videourl",
        "placeholder": "Enter videourl",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    },{
        "name": "Video Thumbnail",
        "type": "image",
        "tableRef": "thumbnail",
        "validation": ["required", "trim", "unique"],
        "url": "",
        "size":"1600 x 899px"
    },{
        "name": "Home Page Banner",
        "type": "image",
        "tableRef": "homebanner",
        "validation": ["required", "trim", "unique"],
        "url": "",
        "size":"1600 x 899px"
    },{
        "name": "Banner Click Url",
        "type": "text",
        "tableRef": "url",
        "placeholder": "Enter url",
        "id": "input5",
        "validation": ["required", "trim", "unique"],
        "url": ""
    }],
    "action": [{
        "name": "submit",
